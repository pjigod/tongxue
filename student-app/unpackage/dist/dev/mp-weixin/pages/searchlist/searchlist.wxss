
.container {
		display: flex;
		width: 100%;
		flex-wrap: wrap;
}
.search-content {
		height: 100rpx;
		width: 100%;
		display: flex;
		align-items: center;
}
.left-icon {
		display: flex;
		height: 60rpx;
		width: 8%;
		margin-left: 15rpx;
}
.searchinput {
		display: flex;
		width: 70%;
		height: 100%;
		justify-content: center;
		align-items: center;
}
.searchtext {
		display: flex;
		height: 100%;
		width: 10%;
		justify-content: center;
		align-items: center;
}
.lines {
		width: 100%;
		height: 2rpx;
		background-color: #e1e1e1;
}


