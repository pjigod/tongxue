
.container {
	display: flex;
	width: 100%;
	flex-wrap: wrap;
}
.top-switch {
	display: flex;
	width: 100%;
	height: 100rpx;
}
.single {
	display: flex;
	width: 50%;
	height: 100%;
	align-items: center;
	justify-content: center;
}
.single-text {
	display: flex;
}
.team {
	display: flex;
	width: 50%;
	height: 100%;
	align-items: center;
	justify-content: center;
}
.team-text {
	display: flex;
}
.temp1 {
	display: flex;
	width: 100%;
	height: 130rpx;
}
.temp2 {
	display: flex;
	width: 100%;
	height: 100rpx;
}
.temp3 {
	display: flex;
	height: 100%;
	width: 25%;
}
.temp4 {
	display: flex;
	height: 15%;
	width: 100%;
}
.clockin-other {
	display: flex;
	width: 100%;
	height: 700rpx;
}
.clockin {
	display: flex;
	width: 50%;
	height: 100%;
	flex-wrap: wrap;
}
.clockin-text {
	display: flex;
	height: 50%;
	width: 100%;
	border-radius: 50%;
	border: 5rpx solid black;
	align-items: center;
	justify-content: center;
}
.inner-text {
	display: flex;
}
.inner-data {}
.other {
	display: flex;
	width: 25%;
	height: 100%;
	flex-wrap: wrap;
}
.ranking {
	display: flex;
	width: 100%;
	height: 33%;
	justify-content: center;
	align-items: center;
}
.schedule {
	display: flex;
	width: 100%;
	height: 33%;
	justify-content: center;
	align-items: center;
}
.record {
	display: flex;
	width: 100%;
	height: 34%;
	justify-content: center;
	align-items: center;
}
.icon {
	display: flex;
	width: 50rpx;
	height: 50rpx;
}
.begin {
	display: flex;
	width: 100%;
	height: 150rpx;
	align-items: center;
	justify-content: center;
}
.begin-button {
	display: flex;
	width: 250rpx;
	height: 100rpx;
}

