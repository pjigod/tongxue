
.container {
	display: flex;
	width: 730rpx;
	justify-content: center;
	align-items: center;
	flex-wrap: wrap;
}
.userInfo {
	display: flex;
	width: 100%;
	height: 200rpx;
	flex-wrap: wrap;
	gap: 70rpx;
	border-radius: 24rpx;
	background-color: white;
}
.userImage {
	display: flex;
	align-items: center;
	flex-wrap: wrap;
	width: 120rpx;
	height: 100%;
	margin-left: 40rpx;
}
.collection-image {
	display: flex;
	align-items: center;
	flex-wrap: wrap;
	width: 100rpx;
	height: 100%;
	margin-left: 20rpx;
}
.inner_image {
	display: flex;
	justify-content: center;
	align-items: center;
	margin-left: 10rpx;
	width: 120rpx;
	height: 100rpx;
	border-radius: 180rpx;
	border: 5rpx solid black;
}
.userName {
	display: flex;
	width: 200rpx;
	height: 100%;
	justify-content: center;
	align-items: center;
}
.userTitle {
